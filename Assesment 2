<!DOCTYPE html>
<html>
  <head>
    <script>
 function validate()
   {
 var pswd = document.getElementById('pass').value;
 var conpass = document.getElementById('conpass').value;
 if(pswd==conpass){}
 else
   {
 alert("Password does not match");
}
}
 function visible()
   {
 if(document.getElementById('vis').checked==true)
   {
 document.getElementById('pass').type="text";
 
}
}


 function fillData()
 {

category=['select','Graduation','Post-Graduation'];


var dd= document.getElementById("items");

for(i=0;i<category.length;i++)
{

var ele=document.createElement('option');
ele.text=category[i];
dd.add(ele,i);
}



}


function fillOpts()
{
opts=[['BCA','BSC','BCOM','BTECH'],['MCA','MTECH','MSC']];


var dd= document.getElementById("items");

var ddOpts= document.getElementById("opts");

ddOpts.innerHTML=null;

idx= dd.selectedIndex;
console.log();

if(idx>0){
 for(i=0;i<opts[idx-1].length;i++)
        {

        var ele1=document.createElement('option');
                ele1.text=opts[idx-1][i];
        ddOpts.add(ele1,i);    
        }
}

}

function dobVal()
  {
    var d=new Date();
    var born = document.getElementById('age').value;
var cur_year = d.getFullYear();
var born_year = born.getFullYear();
if((cur_year-born_year)<18)
 {
   
   document.getElementById('age').min=curr_year-18;
 }
  }


</script>
  </head>
  <body onload="fillData()">
    <h1>Registration Form</h1>
    <form action="jscssass.html">
  <table>
         <tr>
  <td>Password:</td>
  <td><input type="password" name="pswd" id="pass" required></td>
</tr>
<tr>
  <td>Confirm Password</td>
  <td><input type="password" name="conpswd" id="conpass" required></td>
</tr>
<tr>
  <td><input type="date" name="dob" id="age" onclick="dobVal()" required></td>
</tr>
<tr>
  <td>
  <select name="qual" id="items" onchange ="fillOpts()" required>
  </select>
  <select id="opts" required>
  </select>
  </td>
</tr>
<tr>
  <td><input type="checkbox" name="show" id="vis" onclick="visible()" required>Show Password</td>
</tr>
       </table>  
  <input type="submit" name="submit" value="submit" onclick="validate()">
</form>
  </body>
</html>
